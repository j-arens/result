{
  "name": "@j-arens/result",
  "version": "1.0.0",
  "author": "Josh Arens",
  "license": "MIT",
  "description": "A basic port of the Result type from Rust",
  "homepage": "https://github.com/j-arens/result#readme",
  "keywords": [
    "rust",
    "result",
    "typescript"
  ],
  "main": "index.js",
  "types": "Result.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-arens/result.git"
  },
  "bugs": {
    "url": "https://github.com/j-arens/result/issues"
  },
  "scripts": {
    "build": "tsc -d src/index.ts --outDir ./",
    "compile": "npm run test && npm run lint && npm run build",
    "lint": "eslint src/**/*.ts",
    "test": "mocha -r ts-node/register test/index.ts",
    "postversion": "git push && git push --tags",
    "release": "npm run compile && npm version ${NEXT_VERSION=patch} && npm publish --access public"
  },
  "dependencies": {
    "@j-arens/option": "^1.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "mocha": "^10.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
